<p> my page </p>

<section id="top-customers">
    <h3>Top Customers</h3>
    <ol>
        <% @top_five.each do |customer| %>
            <li class='customer'> <%= customer.first_name %> <%= customer.last_name %> - <%= customer.successful_count %> purchases</li>
        <% end %>
    </ol>
</section>

<section id="incomplete-invoices">
    <h3>Incomplete Invoices</h3>
    <ol>
        <% @incomplete_invoices.each do |invoice| %>
            <li> <%= link_to("Invoice ##{invoice.id} #{invoice.clean_date}", admin_invoice_path(invoice.id)) %> </li>
        <% end %>
    </ol>
</section>